{
  "name": "dmhy-download-helper",
  "version": "1.0.0",
  "description": "A dmhy.org download userscript.",
  "private": true,
  "scripts": {
    "serve": "webpack-dev-server",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuoruan/dmhy-download-helper.git"
  },
  "author": "Xingwang Liao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuoruan/dmhy-download-helper/issues"
  },
  "homepage": "https://github.com/kuoruan/dmhy-download-helper#readme",
  "devDependencies": {
    "@vue/eslint-config-prettier": "^4.0.1",
    "autoprefixer": "^9.5.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.17.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "userscript": {
    "name": "DMHY download helper",
    "namespace": "https://github.com/kuoruan",
    "match": [
      "*://share.dmhy.org/",
      "*://share.dmhy.org/topics/list/*",
      "*://share.dmhy.org/topics/list?*"
    ],
    "require": [
      "https://cdn.jsdelivr.net/npm/vue@2.6.8/dist/vue.min.js"
    ],
    "grant": [
      "GM_addStyle",
      "GM_setClipboard"
    ],
    "source": "https://github.com/kuoruan/dmhy-download-helper.git"
  }
}
