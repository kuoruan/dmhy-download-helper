{
  "name": "dmhy-download-helper",
  "version": "1.1.0",
  "description": "A dmhy.org download userscript.",
  "private": true,
  "scripts": {
    "serve": "rollup -c -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuoruan/dmhy-download-helper.git"
  },
  "author": "Xingwang Liao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuoruan/dmhy-download-helper/issues"
  },
  "homepage": "https://github.com/kuoruan/dmhy-download-helper#readme",
  "devDependencies": {
    "@vue/eslint-config-prettier": "^4.0.1",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "postcss-url": "^8.0.0",
    "prettier": "^1.17.0",
    "pug": "^2.0.3",
    "rollup": "^1.12.3",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-eslint": "^6.0.0",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-stylus-compiler": "^1.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "userscript": {
    "name": "DMHY download helper",
    "namespace": "https://github.com/kuoruan",
    "match": [
      "*://share.dmhy.org/",
      "*://share.dmhy.org/topics/list/*",
      "*://share.dmhy.org/topics/list?*"
    ],
    "require": [
      "https://cdn.jsdelivr.net/npm/vue@2.6.8/dist/vue.min.js"
    ],
    "grant": [
      "GM_setClipboard"
    ],
    "source": "https://github.com/kuoruan/dmhy-download-helper.git"
  }
}
